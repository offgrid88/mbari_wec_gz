<?xml version="1.0" ?>
<sdf version="1.7">
    <model name="Hydraulics Test">

      <link name="Buoy">
        <pose relative_to="__model__">0 0 0 0 0 0</pose>
        <inertial>
            <pose>0 0 2.13 0 0 0</pose>
            <mass>1400</mass>
            <inertia>
                <ixx>1429</ixx>
                <ixy>6.77</ixy>
                <ixz>4.69</ixz>
                <iyy>670.31</iyy>
                <iyz>30.5</iyz>
                <izz>1476</izz>
            </inertia>
        </inertial>
        <visual name="visualBuoy">
            <geometry>
                <mesh>
                    <uri>model://mbari_wec/meshes/buoy_float.stl</uri>
                </mesh>
            </geometry>
            <!--color-->
            <material>
                <ambient>1.0 1.0 0.0 1</ambient>
                <diffuse>1.0 1.0 0.0 1</diffuse>
                <specular>1.0 1.0 0.0 1</specular>
            </material>
        </visual>
        <collision name="collision">
            <pose>0 0 2.46 0 0 0 </pose>
            <geometry>
                <box>
                    <size>2.34 2.34 1</size>
                </box>
            </geometry>
        </collision>
     </link>

      <link name="PTO">
        <pose relative_to="Buoy">0 0 0 0 0 0</pose>
        <inertial>
            <pose>0 0 -3.67 0 0 0</pose>
            <mass>605</mass>
            <inertia>
            <ixx>3219</ixx>
            <ixy>-0.43</ixy>
            <ixz>-2.56</ixz>
            <iyy>3219</iyy>
            <iyz>3.37</iyz>
            <izz>7.28</izz>
            </inertia>
        </inertial>
        <visual name="visualPTO">
            <geometry>
                <mesh>
                    <uri>model://mbari_wec/meshes/pto.stl</uri>
                </mesh>
            </geometry>
            <!--color-->
            <material>
                <ambient>.2 .2 0.2 0.5</ambient>
                <diffuse>.2 .2 0.2 0.5</diffuse>
                <specular>.2 .2 0.2 0.5</specular>
            </material>
        </visual>
        <collision name="collision">
            <geometry>
                <mesh>
                    <uri>model://mbari_wec/meshes/pto_collision.stl</uri>
                </mesh>
           </geometry>
        </collision>
      </link>

    <link name="Piston">
      <pose relative_to="PTO">0 0 -3.50066 0 0 0</pose>
      <inertial>
        <mass>48</mass>
        <pose>0 0 -2.57934 0 0 0</pose>
        <inertia>
          <ixx>128</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>128</iyy>
          <iyz>0</iyz>
          <izz>0.0216</izz>
        </inertia>
      </inertial>
      <visual name="visualPiston">
        <geometry>
          <mesh>
            <uri>model://mbari_wec/meshes/rod_and_piston.stl</uri>
          </mesh>
        </geometry>
        <!--color-->
        <material>
          <ambient>0.0 0.0 1.0 1</ambient>
          <diffuse>0.0 0.0 1.0 1</diffuse>
          <specular>0.0 0.0 1.0 1</specular>
        </material>
      </visual>
    </link>
    

    <joint name="Universal" type="fixed">
      <parent>Buoy</parent>
      <child>PTO</child>
      <provide_feedback>1</provide_feedback>
      <pose>0.0 0.0 0.0 0 0 0</pose>
    </joint>

    <joint name="HydraulicRam" type="prismatic">
      <parent>PTO</parent>
      <child>Piston</child>
      <pose>0.0 0.0 0.0 0 0 0</pose>
      <axis>
        <limit>
          <lower>0.0</lower>
          <upper>2.03</upper>
        </limit>
        <xyz>0.0 0.0 1.0</xyz>
      </axis>
    </joint>

    <plugin
        filename="ignition-gazebo-joint-state-publisher-system"
        name="ignition::gazebo::systems::JointStatePublisher">
    </plugin>
    </model>
</sdf>
